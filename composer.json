    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ismail1k\\LaravelStatistics\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ismail Andaloussi",
            "email": "oneigcy@gmail.com"
        }
